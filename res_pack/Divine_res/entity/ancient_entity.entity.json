{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "divine:ancient_entity",
      "materials": {
        "default": "divine"
      },
      "textures": {
        "default": "textures/entity/ancient_entity"
      },
      "geometry": {
        "default": "geometry.ancient_entity"
      },
        "animations": {
        "move": "animation.ancient_entity.move",   
        "look_at_target": "animation.common.look_at_target" 
      }, 
      "animation_controllers": [
        { "move": "controller.animation.ancient_entity.move" }
      ],
      "scripts": {
        "animate": [
          "move"
        ]
      },
      "render_controllers": [ "controller.render.ancient_entity" ],
       "spawn_egg": {
        "overlay_color": "#12800",
        "base_color": "#101013"
      }
    }
  }
}